{% extends '_mdb/mdb_base.html.twig' %}

{% block form_start %}
    {{ form_start(form) }}
{% endblock %}

{% block pagetop_fluid %}
    {{ include ("_mdb/mdb_page_top_section.html.twig", {
        'title' : 'Datos del tipo de equipo',
        'link' : {
            'back' : path('equipo_tipo_index'),
            'submit' : 'Confirmar'
        }
    }) }}
{% endblock %}

{% block body_fluid %}
    <div class="white">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                <div class="clearfix">
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 md-form">{{ form_row(form.nombre) }}</div>
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 md-form">{{ form_row(form.descripcion) }}</div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

{% block form_end %}
    {{ form_end(form) }}
{% endblock %}
