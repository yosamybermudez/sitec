<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">    <meta http-equiv="x-ua-compatible" content="ie=edge">    <title>{{ system_title }}</title>    <!-- MDB icon -->    {%  set asset = asset('') %}    <!-- Font Awesome -->    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/fonts/fontawesome/fontawesome.css') }}">    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/fonts/glyphicons/glyphicons.css') }}">    <!-- Bootstrap core CSS -->    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/css/bootstrap4.min.css') }}">    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/mdb4pro/css/bootstrap.min.css') }}">    <!-- Material Design Bootstrap -->    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/mdb4pro/css/mdb.min.css') }}">    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/mdb4pro/css/mdb.min.css') }}">    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/institucional/colors.css') }}">    {#    <link rel="stylesheet" href="{{ asset('assets/mdb4pro/css/mdb.lite.min.css') }}">#}    {#    <!-- Colores institucionales -->#}    {#    <link rel="stylesheet" href="{{ asset('assets/institucional/colors.css') }}">#}    <!-- Datatables -->    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/mdb4pro/css/addons/datatables.min.css') }}">    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/mdb4pro/css/addons/datatables-select.min.css') }}">    <!-- Your custom styles (optional) -->    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/mdb4pro/css/styles-all.css') }}">    <link rel="stylesheet" href="{{ app.request.schemeAndHttpHost }}{{ asset('assets/css/other/spinner.css') }}">    {% block head %} {% endblock %}    <style>        .bold{            font-weight: bold;            text-align: right;            padding-right: 10px;        }        td:not(.bold){            padding-left: 10px;        }        table{            font-family: "Arial";        }        .text-blue-sitec {            color: rgb(2, 37, 75);        }    </style></head><body><!-- Start your project here--><div>    {% set footer = 'Servicios Integrales Tecnológicos – Esquina de Tejas entre 10 de octubre y San Joaquín – Teléfono: 78795048' %}    <table class="table-bordered mb-3" style="background-color: white; table-layout: fixed; width: 100%;">        <tr>            <td rowspan="3" colspan="1" class="p-3 pb-0">                <img src="{{ app.request.schemeAndHttpHost }}{{ asset('assets/institucional/sitec-logo-horizontal.jpg') }}" style="width: 100%;">            </td>            <td rowspan="3" colspan="4" class="text-blue-sitec p-3 pb-0" style="font-weight: bold; text-transform: uppercase; font-size: 20px; text-align: center;">                <h3>{{ title }}</h3>            </td>        </tr>    </table>    <div class="text-center mb-3 border-bottom p-1 institucional-text"><small>{{ footer }}</small></div>    {% block document %}    {% endblock %}    </div></div><!-- End your project here--></body></html>